{
  "name": "translate-kit",
  "version": "0.3.0",
  "description": "AI-powered translation SDK for build time. Compatible with next-intl.",
  "type": "module",
  "license": "MIT",
  "author": "guillermolg00",
  "repository": {
    "type": "git",
    "url": "https://github.com/guillermolg00/translate-kit.git"
  },
  "homepage": "https://translate-kit.com",
  "bugs": "https://github.com/guillermolg00/translate-kit/issues",
  "keywords": [
    "translate",
    "i18n",
    "internationalization",
    "next-intl",
    "ai",
    "translation",
    "build-time",
    "vercel-ai-sdk",
    "localization"
  ],
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "translate-kit": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@babel/generator": "^7.29.1",
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0",
    "@clack/prompts": "^1.0.0",
    "c12": "^4.0.0-beta.2",
    "citty": "^0.2.0",
    "dotenv": "^17.2.4",
    "p-limit": "^7.3.0",
    "picocolors": "^1.1.1",
    "tinyglobby": "^0.2.15",
    "tokenlens": "^1.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^25.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "ai": ">=4.0.0"
  }
}
